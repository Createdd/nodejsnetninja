<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Test Site</title>
    <style>
      body{background:lightgreen; color:red; text-align:center;}
      h1, h2 {color:green;}
    </style>
  </head>

  <body>
    <h1>Welcome to the profile of <%=person%></h1>
    <p><strong>Age: </strong><%=data.age%></p>
    <p><strong>Job: </strong><%=data.job%></p>
    <h2>Hobbies</h2>
    <ul>
      <%data.hobbies.forEach(function(item){%>
        <li><%=item%></li>
      <%});%>
    </ul>
  </body>
</html>
